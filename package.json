{
  "name": "device-activity-tracker",
  "version": "1.0.0",
  "type": "module",
  "description": "A proof-of-concept tool demonstrating privacy vulnerabilities in messaging apps through RTT-based activity analysis",
  "main": "dist/index.js",
  "keywords": [
    "whatsapp",
    "signal",
    "privacy",
    "security",
    "research",
    "tracking",
    "rtt",
    "device-activity",
    "proof-of-concept"
  ],
  "author": "KH178",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KH178/Adaptive-RTT-Measurement-System-for-device-Tracking.git"
  },
  "bugs": {
    "url": "https://github.com/KH178/Adaptive-RTT-Measurement-System-for-device-Tracking/issues"
  },
  "homepage": "https://github.com/KH178/Adaptive-RTT-Measurement-System-for-device-Tracking#readme",
  "scripts": {
    "build": "tsc",
    "start": "npx tsx src/index.ts",
    "start:server": "npm run start:signal-api && npx tsx src/server.ts",
    "start:signal-api": "npx tsx src/scripts/init-signal.ts",
    "start:client": "cd client && npm start",
    "dev": "npm run start:server",
    "postinstall": "cd client && npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "better-sqlite3": "^12.6.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.0.1",
    "pdf-parse": "^1.1.1",
    "pino": "^10.1.0",
    "qrcode-terminal": "^0.12.0",
    "socket.io": "^4.8.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/ws": "^8.18.1",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  },
  "private": true
}
